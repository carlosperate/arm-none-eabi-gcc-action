{
  "name": "arm-none-eabi-gcc-action",
  "version": "1.5.0",
  "private": true,
  "description": "GitHub Action to install arm-none-eabi-gcc",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --config config/prettierrc.json --write **/*.ts",
    "format-check": "prettier --config config/prettierrc.json --check **/*.ts",
    "lint": "eslint --config config/eslintrc.json src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --config config/jest.config.js",
    "all": "npm run build && npm run format-check && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlosperate/arm-none-eabi-gcc-action.git"
  },
  "keywords": [
    "actions",
    "gcc",
    "arm",
    "arm-none-eabi-gcc"
  ],
  "author": "Carlos Pereira Atencio",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^1.0.7",
    "@actions/core": "^1.9.1",
    "@actions/tool-cache": "^1.7.1",
    "md5-file": "^5.0.0",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/parser": "^5.3.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.1.0",
    "eslint-plugin-github": "^4.3.3",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "jest-circus": "^27.3.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
