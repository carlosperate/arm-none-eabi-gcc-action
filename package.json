{
  "name": "arm-none-eabi-gcc-action",
  "version": "1.1.0",
  "private": true,
  "description": "GitHub Action to install arm-none-eabi-gcc",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format-check && npm run lint && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlosperate/arm-none-eabi-gcc-action.git"
  },
  "keywords": [
    "actions",
    "gcc",
    "arm",
    "arm-none-eabi-gcc"
  ],
  "author": "Carlos Pereira Atencio",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/tool-cache": "^1.7.1",
    "@types/tmp": "^0.1.0",
    "md5-file": "^5.0.0",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.28",
    "@types/node-fetch": "^2.5.4",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.13.1",
    "node-fetch": "^2.6.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
